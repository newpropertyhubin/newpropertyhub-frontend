    "chalk": "^5.3.0",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv": "^16.3.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^7.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^2.2.1",
    "next": "^14.2.5",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.11",
    "razorpay": "^2.9.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "nodemon": "^3.0.1"
  },
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cross-env PORT=5000 nodemon",
    "dev:frontend": "next dev -p 3003",
    "build:frontend": "next build",
    "start:frontend": "next start",
    "merge-data": "node seo-generator/merge-data.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:all": "node start-dev.js",
    "dev:win": "start cmd /k npm run dev:backend && start cmd /k npm run dev:frontend",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js"
  },
